        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/contracts": "^11.0",
        "illuminate/database": "^11.0",
        "illuminate/routing": "^11.0",
        "illuminate/support": "^11.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^6.0",
        "laravel/pint": "^1.2",
        "phpunit/phpunit": "^10.0 || ^11.0"
    },
    "type": "library"
