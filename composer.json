        "psr-4": {
            "Butler\\Health\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Butler\\Health\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "description": "Glesys Butler Health package.",
